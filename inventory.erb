[OSEv3:children]
masters
nodes

[OSEv3:vars]
ansible_ssh_user=root
deployment_type=origin
openshift_master_identity_providers=[{'name': 'htpasswd_auth', 'login': 'true', 'challenge': 'true', 'kind': 'HTPasswdPasswordIdentityProvider', 'filename': '/etc/origin/master/htpasswd'}]
containerized=true
openshift_release=v1.5
openshift_image_tag=v1.5.0
openshift_public_hostname=oso-console.colinhenry.co.uk
openshift_master_default_subdomain=apps.oso-console.colinhenry.co.uk
openshift_hosted_metrics_deploy=false


[masters]
oso-console.colinhenry.co.uk openshift_schedulable=true

[nodes]
oso-console.colinhenry.co.uk openshift_node_labels="{'region': 'infra', 'zone': 'default'}" openshift_schedulable=true